{
  "cli": {
    "version": ">= 12.6.2",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_GOOGLE_API_KEY": "EXPO_PUBLIC_GOOGLE_API_KEY",
        "GOOGLE_SERVICES_FILE": "googleServicesFile",
        "EXPO_PUBLIC_WEB_CLIENT_ID": "EXPO_PUBLIC_WEB_CLIENT_ID"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_GOOGLE_API_KEY": "EXPO_PUBLIC_GOOGLE_API_KEY",
        "GOOGLE_SERVICES_FILE": "googleServicesFile",
        "EXPO_PUBLIC_WEB_CLIENT_ID": "EXPO_PUBLIC_WEB_CLIENT_ID"
      }
    },
    "production": {
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_GOOGLE_API_KEY": "EXPO_PUBLIC_GOOGLE_API_KEY",
        "GOOGLE_SERVICES_FILE": "googleServicesFile",
        "EXPO_PUBLIC_WEB_CLIENT_ID": "EXPO_PUBLIC_WEB_CLIENT_ID"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
